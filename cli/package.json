    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8"
  }
}
