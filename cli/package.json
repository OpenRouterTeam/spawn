{
  "name": "@openrouter/spawn",
  "version": "0.2.34",
  "type": "module",
  "bin": {
    "spawn": "cli.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outfile cli.js --target bun --minify --packages bundle",
